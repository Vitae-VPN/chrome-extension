{
    "name": "Vitae Proxy",
    "version": "1.1",
    "description": "This extension will proxy all of your internet traffic to a remote server. No requests are logged.",

    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },

    "permissions": [
        "storage",
        "declarativeContent",
        "proxy"
    ],

    "browser_action": {
        "default_popup": "web.html",
        "default_icon": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "192": "icons/192.png",
            "512": "icons/512.png"
        }
    },

    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "192": "icons/192.png",
        "512": "icons/512.png"
    },

    "manifest_version": 2
}